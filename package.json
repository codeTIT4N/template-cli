{
  "name": "template-cli",
  "version": "1.0.0",
  "description": "A CLI app to easily create new projects with some starter code",
  "main": "src/cli.ts",
  "type": "module",
  "scripts": {
    "dev": "npx ts-node src/cli.ts",
    "build": "tsc && sed -i '' \"s/ts-node/node/\" dist/cli.js",
    "start": "node dist/cli.js",
    "reinstall": "npm run build && npm uninstall -g template-cli && npm install -g",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "template-cli": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeTIT4N/template-cli.git"
  },
  "author": "Lokesh Kumar (codeTIT4N)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/codeTIT4N/template-cli/issues"
  },
  "homepage": "https://github.com/codeTIT4N/template-cli#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "inquirer": "^9.1.5",
    "ts-node": "10.9.1"
  }
}
